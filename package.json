{
  "name": "bandicoot",
  "version": "3.0.2",
  "description": "React rich text editor",
  "main": "dist/bandicoot.umd.js",
  "module": "dist/bandicoot.esm.js",
  "repository": "git@github.com:CanopyTax/bandicoot.git",
  "author": "Joel Denning <joeldenning@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "typings"
  ],
  "types": "typings/bandicoot.d.ts",
  "keywords": [
    "react",
    "rich",
    "text",
    "editor"
